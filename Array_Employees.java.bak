class Emp
{
	int emp_id;
	String emp_name;
	String emp_dep;
	String emp_project;
	int emp_hours;
	int emp_salary;
Emp(int id,String name,String dep,String pro_working_on,int hours,int salary)
{
	this.emp_id=id;
	this.emp_name=name;
	this.emp_dep=dep;
	this.emp_project=pro_working_on;
	this.emp_hours=hours;
	this.emp_salary=salary;
}
void display()
{
	System.out.println("Employee id : "+emp_id);
	System.out.println("Employee name : "+emp_name);
	System.out.println("Employee Department : "+emp_dep);
	System.out.println("The employee working on  project : "+emp_project);
	System.out.println("No.of hours the employee work : "+emp_hours);
	System.out.println("Salary of Employee : "+emp_salary);
}
}