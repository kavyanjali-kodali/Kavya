import java.util.Scanner;
class Array_Employees
{
 public static void main(String[] args)
 {
	 Emp []ed=new Emp[3];
	 ed[0]=new Emp(9819,"Ramya Sri","CSE","JAVA",8,100000);
	 ed[1]=new Emp(9513,"Kavyanjali","CSE","PYTHON",8,100000);
